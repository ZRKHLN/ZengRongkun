#total = 0
#count = 0
#average = 0
#input_num = []
min = None
max = None
while True:
    num = input("please input number")
    if num =='done':
        break;
    try:
        num = float(num)
    except:
        print("Invalid input")
    if max is None or max < num:
        max = num
    if min is None or min > num:
        min = num
#    for i in input_num:
#       total + = i
#       count + = 1
#average = total/count
#print(total, count, average)
print(max, min)
